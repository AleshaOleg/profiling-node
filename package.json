{
  "name": "@sentry/profiler-node",
  "version": "0.0.0",
  "description": "Sampling based nodejs profiler",
  "main": "./lib/index.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "lib/*",
    "build/release/cpu_profiler.node"
  ],
  "scripts": {
    "postinstall": "npm run build:bindings",
    "clean": "rm -rf ./lib && rm -rf build",
    "build": "npm run build:bindings && npm run build:lib",
    "build:lib": "tsc",
    "build:bindings": "node-gyp rebuild",
    "build:benchmark:format": "node-gyp -DFORMAT_BENCHMARK=1 rebuild",
    "benchmark": "npm run benchmark:methods && npm run benchmark:profiler && npm run benchmark:server && npm run benchmark:format",
    "benchmark:methods": "node benchmarks/cpu/benchmark.methods.js",
    "benchmark:profiler": "node benchmarks/cpu/benchmark.profiler.js",
    "benchmark:server": "node benchmarks/cpu/benchmark.server.js",
    "benchmark:format": "yarn build:benchmark:format && node benchmarks/format/benchmark.format.js",
    "test": "npm run test"
  },
  "keywords": [
    "profiling",
    "monitoring",
    "cpu",
    "node"
  ],
  "author": "jonas.badalic@sentry.io",
  "license": "ISC",
  "peerDependencies": {
    "@sentry/hub": "^7.10.0",
    "@sentry/node": "^7.10.0",
    "@sentry/types": "^7.8.1",
    "@sentry/utils": "^7.10.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.2",
    "autocannon": "^7.9.0",
    "benchmark": "^2.1.4",
    "express": "^4.18.1",
    "nan": "^2.16.0",
    "node-gyp": "^9.0.0",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simple-zstd": "^1.4.0",
    "sqlite3": "^5.0.8",
    "typescript": "^4.7.4"
  }
}
